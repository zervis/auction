<ul>
  <%= link to: Routes.item_path(@conn, :new) do %>
    add new
  <% end %>
  <%= for item <- @items do %>
  <li><strong><%= link(item.title, to: Routes.item_path(@conn, :show, item)) %></strong>: <%= item.description %> <%= link "Edit", to: Routes.item_path(@conn, :edit, item) %></li>
  <% end %>
</ul>
